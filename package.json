{
  "name": "hsp-fo-search",
  "version": "1.8.50",
  "description": "Search data of the Handschriftenportal",
  "license": "MIT",
  "files": [
    "dist/",
    "declaration/",
    "types.ts"
  ],
  "scripts": {
    "clean": "shx rm -rf ./node_modules ./dist ./test-reports ./target ./generated-types ./declaration *.tgz",
    "fix": "npm run fix:prettier && npm run fix:eslint",
    "fix:eslint": "eslint --fix \"./{src,test}/**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "fix:prettier": "prettier --write \"./{src,test,server}/**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint \"./{src,test}/**/*\"",
    "lint:prettier": "prettier --check \"./{src,test}/**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "test": "jest --clearCache && jest",
    "test:light": "jest --clearCache && jest --coverage=false",
    "build": "webpack --mode=production && tsc --project tsconfig.declaration.json",
    "build:dev": "webpack --mode=development && tsc --project tsconfig.declaration.json",
    "dev": "MODE=development node server/start.js",
    "deb": "echo not in use",
    "version": "echo $npm_package_version | tr -d '\n'",
    "artefactid": "echo $npm_package_name | tr -d '\n'",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/handschriftenportal-dev/hsp-fo-search"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@rsql/builder": "^1.6.0",
    "@rsql/emitter": "^1.6.0",
    "@rsql/parser": "^1.6.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^11.2.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/css-mediaquery": "^0.1.0",
    "@types/express": "^4.17.11",
    "@types/http-proxy": "^1.17.6",
    "@types/jest": "^29.5.11",
    "@types/node-fetch": "^2.5.10",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-virtualized": "^9.21.22",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.6",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^6.7.1",
    "css-mediaquery": "^0.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "msw": "^0.35.0",
    "node-fetch": "^2.6.1",
    "prettier": "^3.1.1",
    "prettier-plugin-sort-json": "^3.1.0",
    "react-virtualized": "^9.22.5",
    "redux-devtools-extension": "^2.13.9",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.1.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "wait-on": "^7.0.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.2.1",
    "webpack-hot-middleware": "^2.26.1"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.0.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@reduxjs/toolkit": "^1.9.5",
    "clsx": "^2.1.0",
    "hsp-web-module": "^1.30.4",
    "notistack": "^1.0.10",
    "qs": "^6.5.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.18.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "schema-dts": "^1.1.0",
    "url-join": "^2.0.5"
  }
}
